@model DepartmentVM
@{
    ViewData["Title"] = "Delete";
}


<div class="block-header">
    <h2>@Model.Name Delete</h2>
</div>


<form class="form-group" asp-action="Delete" asp-controller="Department">

    <input type="hidden" asp-for="Id" />

    @{
        await Html.RenderPartialAsync("~/Views/Partials/Department/DetailsDelete.cshtml");
    }

    <center>
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-controller="Department" asp-action="Index"  class="btn btn-warning">Return</a>

    </center>

</form>